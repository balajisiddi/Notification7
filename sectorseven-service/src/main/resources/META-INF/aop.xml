<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE project>
<!-- AspectJ load-time weaving options -->
<aspectj>

  <!-- Use -showWeaveInfo option to log what is being woven and load time -->
  <weaver options="-Xset:weaveJavaxPackages=true -showWeaveInfo">
      <include within="com.sectorseven..*"/>
      <!-- Dump the generated classes for debugging:
      <dump  within="com.real..*"/>
      -->
  </weaver>

</aspectj>
